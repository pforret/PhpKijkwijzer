{
    "name": "pforret/pf_kijkwijzer",
    "version": "1.1.1",
    "description": "PHP Component to retrieve Kijkwijzer (NL/BE) ratings for movies",
    "keywords": [
        "pforret",
        "PhpKijkwijzer"
    ],
    "homepage": "https://github.com/pforret/pf_kijkwijzer.git",
    "license": "MIT",
    "authors": [
        {
            "name": "pforret",
            "email": "peter@forret.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "pforret/pf_pageparser": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "Pforret\\PfKijkwijzer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pforret\\PfKijkwijzer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "./vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
